{
  "name": "@bbgse/logsome",
  "version": "0.0.1",
  "packageManager": "pnpm@9.1.0",
  "description": "A simple logging library for Node.js",
  "author": "",
  "license": "ISC",
  "keywords": [],
  "exports": {
    ".": "./dist/index.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "build": "tsup",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "es-toolkit": "1.30.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.12.0",
    "@types/node": "^22.10.2",
    "@vitest/coverage-v8": "2.1.8",
    "eslint": "^9.16.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "dts": true,
    "splitting": true,
    "sourcemap": true,
    "clean": true
  }
}
